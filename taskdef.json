{
    "executionRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/allu-cicd-role",
    "containerDefinitions": [
        {
            "name": "allu-web",
            "image": "<IMAGE1_NAME>",
            "logConfiguration":
            {
                "logDriver": "awslogs",
                "options":
                {
                    "awslogs-group": "/ecs/logs/WebAppGroups",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "memory": null,
            "memoryReservation": null,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "<ContainerCpuUnit>",
    "memory": "<ContainerMemory>",
    "family": "webtest",
    "taskRoleArn": "<EcsRoleArn>"
}
